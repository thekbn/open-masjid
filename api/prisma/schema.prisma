generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model iqamah {
  id                  Int       @id @default(autoincrement())
  masjid_id           Int
  salah               String
  time                DateTime? @db.Time(6)
  masjids             masjids   @relation(fields: [masjid_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  salah_iqamahTosalah salah     @relation(fields: [salah], references: [name], onDelete: NoAction, onUpdate: NoAction)
}

model masjids {
  id      Int      @id @default(autoincrement())
  name    String
  address String?
  iqamah  iqamah[]
}

model salah {
  name        String   @id
  description String?
  iqamah      iqamah[]
}
